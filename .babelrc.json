{
  "plugins": [
    "@babel/plugin-transform-typescript",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-regenerator"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "11",
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      },
      "@babel/preset-typescript"
    ]
  ]
